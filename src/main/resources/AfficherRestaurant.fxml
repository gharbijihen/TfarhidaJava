        </TableView>
      <TextField fx:id="nom" layoutX="673.0" layoutY="88.0" promptText="nom" />
      <TextField fx:id="adresse" layoutX="673.0" layoutY="148.0" promptText="adresse" />
      <TextField fx:id="numdetel" layoutX="672.0" layoutY="209.0" promptText="num de tel" />
