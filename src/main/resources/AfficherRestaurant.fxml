                <TableColumn fx:id="nomCol" prefWidth="84.0" text="Nom" />
                <TableColumn fx:id="adresseCol" prefWidth="100.0" text="adresse" />
                <TableColumn fx:id="numdetelCol" prefWidth="99.0" text="num de tel" />
            <TableColumn fx:id="nmbetoilesCol" prefWidth="84.0" text="nmb d'etoiles" />
